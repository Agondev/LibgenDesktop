<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="DownloadDumpLinksPanel" TargetType="StackPanel">
        <Setter Property="Orientation" Value="Vertical" />
    </Style>
    <Style x:Key="Header" TargetType="TextBlock">
        <Setter Property="Margin" Value="12,0,12,12" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>
    <Style x:Key="OpenPagePanel" TargetType="StackPanel">
        <Setter Property="Orientation" Value="Horizontal" />
        <Setter Property="Margin" Value="0,0,0,8" />
    </Style>
    <Style x:Key="OpenPage" TargetType="TextBlock">
        <Setter Property="Margin" Value="0,0,3,0" />
    </Style>
    <Style x:Key="FileList" TargetType="ItemsControl">
        <Setter Property="Margin" Value="0,8,0,0" />
    </Style>
    <Style x:Key="FilePanel" TargetType="StackPanel">
        <Setter Property="Orientation" Value="Vertical" />
        <Setter Property="Margin" Value="0,0,0,8" />
    </Style>
    <Style x:Key="CollectionName" TargetType="TextBlock">
        <Setter Property="FontWeight" Value="Medium" />
        <Setter Property="Foreground" Value="#707070" />
        <Setter Property="Margin" Value="0,0,0,4" />
    </Style>
</ResourceDictionary>